#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/opt/lampp/htdocs/wordpress/wp-content/themes/jadoo/node_modules/.pnpm/nopt@5.0.0/node_modules/nopt/bin/node_modules:/opt/lampp/htdocs/wordpress/wp-content/themes/jadoo/node_modules/.pnpm/nopt@5.0.0/node_modules/nopt/node_modules:/opt/lampp/htdocs/wordpress/wp-content/themes/jadoo/node_modules/.pnpm/nopt@5.0.0/node_modules:/opt/lampp/htdocs/wordpress/wp-content/themes/jadoo/node_modules/.pnpm/node_modules:/opt/lampp/htdocs/wordpress/wp-content/themes/jadoo/node_modules:/opt/lampp/htdocs/wordpress/wp-content/themes/node_modules:/opt/lampp/htdocs/wordpress/wp-content/node_modules:/opt/lampp/htdocs/wordpress/node_modules:/opt/lampp/htdocs/node_modules:/opt/lampp/node_modules:/opt/node_modules:/node_modules:/opt/lampp/htdocs/wordpress/wp-content/themes/jadoo/node_modules/.pnpm/node-gyp@8.4.1/node_modules/node-gyp/node_modules:/opt/lampp/htdocs/wordpress/wp-content/themes/jadoo/node_modules/.pnpm/node-gyp@8.4.1/node_modules"
else
  export NODE_PATH="$NODE_PATH:/opt/lampp/htdocs/wordpress/wp-content/themes/jadoo/node_modules/.pnpm/nopt@5.0.0/node_modules/nopt/bin/node_modules:/opt/lampp/htdocs/wordpress/wp-content/themes/jadoo/node_modules/.pnpm/nopt@5.0.0/node_modules/nopt/node_modules:/opt/lampp/htdocs/wordpress/wp-content/themes/jadoo/node_modules/.pnpm/nopt@5.0.0/node_modules:/opt/lampp/htdocs/wordpress/wp-content/themes/jadoo/node_modules/.pnpm/node_modules:/opt/lampp/htdocs/wordpress/wp-content/themes/jadoo/node_modules:/opt/lampp/htdocs/wordpress/wp-content/themes/node_modules:/opt/lampp/htdocs/wordpress/wp-content/node_modules:/opt/lampp/htdocs/wordpress/node_modules:/opt/lampp/htdocs/node_modules:/opt/lampp/node_modules:/opt/node_modules:/node_modules:/opt/lampp/htdocs/wordpress/wp-content/themes/jadoo/node_modules/.pnpm/node-gyp@8.4.1/node_modules/node-gyp/node_modules:/opt/lampp/htdocs/wordpress/wp-content/themes/jadoo/node_modules/.pnpm/node-gyp@8.4.1/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../nopt@5.0.0/node_modules/nopt/bin/nopt.js" "$@"
else
  exec node  "$basedir/../../../../../nopt@5.0.0/node_modules/nopt/bin/nopt.js" "$@"
fi
