#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/opt/lampp/htdocs/wordpress/wp-content/themes/jadoo/node_modules/.pnpm/webpack@5.66.0_webpack-cli@4.9.1/node_modules/webpack/bin/node_modules:/opt/lampp/htdocs/wordpress/wp-content/themes/jadoo/node_modules/.pnpm/webpack@5.66.0_webpack-cli@4.9.1/node_modules/webpack/node_modules:/opt/lampp/htdocs/wordpress/wp-content/themes/jadoo/node_modules/.pnpm/webpack@5.66.0_webpack-cli@4.9.1/node_modules:/opt/lampp/htdocs/wordpress/wp-content/themes/jadoo/node_modules/.pnpm/node_modules:/opt/lampp/htdocs/wordpress/wp-content/themes/jadoo/node_modules:/opt/lampp/htdocs/wordpress/wp-content/themes/node_modules:/opt/lampp/htdocs/wordpress/wp-content/node_modules:/opt/lampp/htdocs/wordpress/node_modules:/opt/lampp/htdocs/node_modules:/opt/lampp/node_modules:/opt/node_modules:/node_modules:/opt/lampp/htdocs/wordpress/wp-content/themes/jadoo/node_modules/.pnpm/babel-loader@8.2.3_webpack@5.66.0/node_modules/babel-loader/node_modules:/opt/lampp/htdocs/wordpress/wp-content/themes/jadoo/node_modules/.pnpm/babel-loader@8.2.3_webpack@5.66.0/node_modules"
else
  export NODE_PATH="$NODE_PATH:/opt/lampp/htdocs/wordpress/wp-content/themes/jadoo/node_modules/.pnpm/webpack@5.66.0_webpack-cli@4.9.1/node_modules/webpack/bin/node_modules:/opt/lampp/htdocs/wordpress/wp-content/themes/jadoo/node_modules/.pnpm/webpack@5.66.0_webpack-cli@4.9.1/node_modules/webpack/node_modules:/opt/lampp/htdocs/wordpress/wp-content/themes/jadoo/node_modules/.pnpm/webpack@5.66.0_webpack-cli@4.9.1/node_modules:/opt/lampp/htdocs/wordpress/wp-content/themes/jadoo/node_modules/.pnpm/node_modules:/opt/lampp/htdocs/wordpress/wp-content/themes/jadoo/node_modules:/opt/lampp/htdocs/wordpress/wp-content/themes/node_modules:/opt/lampp/htdocs/wordpress/wp-content/node_modules:/opt/lampp/htdocs/wordpress/node_modules:/opt/lampp/htdocs/node_modules:/opt/lampp/node_modules:/opt/node_modules:/node_modules:/opt/lampp/htdocs/wordpress/wp-content/themes/jadoo/node_modules/.pnpm/babel-loader@8.2.3_webpack@5.66.0/node_modules/babel-loader/node_modules:/opt/lampp/htdocs/wordpress/wp-content/themes/jadoo/node_modules/.pnpm/babel-loader@8.2.3_webpack@5.66.0/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../webpack@5.66.0_webpack-cli@4.9.1/node_modules/webpack/bin/webpack.js" "$@"
else
  exec node  "$basedir/../../../../../webpack@5.66.0_webpack-cli@4.9.1/node_modules/webpack/bin/webpack.js" "$@"
fi
