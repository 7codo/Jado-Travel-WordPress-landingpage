#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/opt/lampp/htdocs/wordpress/wp-content/themes/jadoo/node_modules/.pnpm/svgo@2.8.0/node_modules/svgo/bin/node_modules:/opt/lampp/htdocs/wordpress/wp-content/themes/jadoo/node_modules/.pnpm/svgo@2.8.0/node_modules/svgo/node_modules:/opt/lampp/htdocs/wordpress/wp-content/themes/jadoo/node_modules/.pnpm/svgo@2.8.0/node_modules:/opt/lampp/htdocs/wordpress/wp-content/themes/jadoo/node_modules/.pnpm/node_modules:/opt/lampp/htdocs/wordpress/wp-content/themes/jadoo/node_modules:/opt/lampp/htdocs/wordpress/wp-content/themes/node_modules:/opt/lampp/htdocs/wordpress/wp-content/node_modules:/opt/lampp/htdocs/wordpress/node_modules:/opt/lampp/htdocs/node_modules:/opt/lampp/node_modules:/opt/node_modules:/node_modules:/opt/lampp/htdocs/wordpress/wp-content/themes/jadoo/node_modules/.pnpm/postcss-svgo@5.0.3_postcss@8.4.5/node_modules/postcss-svgo/node_modules:/opt/lampp/htdocs/wordpress/wp-content/themes/jadoo/node_modules/.pnpm/postcss-svgo@5.0.3_postcss@8.4.5/node_modules"
else
  export NODE_PATH="$NODE_PATH:/opt/lampp/htdocs/wordpress/wp-content/themes/jadoo/node_modules/.pnpm/svgo@2.8.0/node_modules/svgo/bin/node_modules:/opt/lampp/htdocs/wordpress/wp-content/themes/jadoo/node_modules/.pnpm/svgo@2.8.0/node_modules/svgo/node_modules:/opt/lampp/htdocs/wordpress/wp-content/themes/jadoo/node_modules/.pnpm/svgo@2.8.0/node_modules:/opt/lampp/htdocs/wordpress/wp-content/themes/jadoo/node_modules/.pnpm/node_modules:/opt/lampp/htdocs/wordpress/wp-content/themes/jadoo/node_modules:/opt/lampp/htdocs/wordpress/wp-content/themes/node_modules:/opt/lampp/htdocs/wordpress/wp-content/node_modules:/opt/lampp/htdocs/wordpress/node_modules:/opt/lampp/htdocs/node_modules:/opt/lampp/node_modules:/opt/node_modules:/node_modules:/opt/lampp/htdocs/wordpress/wp-content/themes/jadoo/node_modules/.pnpm/postcss-svgo@5.0.3_postcss@8.4.5/node_modules/postcss-svgo/node_modules:/opt/lampp/htdocs/wordpress/wp-content/themes/jadoo/node_modules/.pnpm/postcss-svgo@5.0.3_postcss@8.4.5/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../svgo@2.8.0/node_modules/svgo/bin/svgo" "$@"
else
  exec node  "$basedir/../../../../../svgo@2.8.0/node_modules/svgo/bin/svgo" "$@"
fi
